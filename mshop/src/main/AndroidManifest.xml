<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.client">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".app.MyApp"
        android:theme="@style/Theme.MyApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.test.DemoActivity"></activity>
        <activity android:name=".SplaceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.test.TestActivity"
            android:theme="@style/ActivityBackground"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ui.home.ChannelListActivity"/>
        <activity android:name=".ui.home.HotGoodActivity"/>
        <activity android:name=".ui.shop.CarActivity"/>
        <activity android:name=".ui.login.LoginActivity"/>
        <activity android:name=".ui.shop.OrderActivity"/>
        <activity android:name=".ui.me.UserInfoDetailActivity"
            android:windowSoftInputMode="adjustResize"/>
    </application>

</manifest>